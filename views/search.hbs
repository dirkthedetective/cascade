<div class="box">
    <div class="title">
        <img src="/images/w98_help_book_small.ico" width="18" height="18" class="title" />
        <p class="title">Search Results</p>
        <button id="logout-button">X</button>
        <button>?</button>
    </div>
    <div class="body">
        <div>Strong matches</div>
        <div id="orders" class="body">
            {{#each items}}
            <div class="order-box">
                <div class="order-pos">
                    <ul>
                        <li><a href="/item/{{_id}}"><img class="logo2" src={{image}}></a></li>
                        <li>{{title}}</li>
                        <li>{{type}}</li> 
                    </ul>
                </div>
            </div> {{/each}}
        </div>
        <div>Weak matches</div>
        <div id="orders" class="body">
            {{#each items2}}
            <div class="order-box">
                <div class="order-pos">
                    <ul>
                        <li><a href="/item/{{_id}}"><img class="logo2" src={{image}}></a></li>
                        <li>{{title}}</li>
                        <li>{{type}}</li> 
                    </ul>
                </div>
            </div> {{/each}}
        </div>
    </div>
</div>